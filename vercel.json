{
  "version": 2,
  "builds": [
    {
      "src": "Ecomb/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Ecomb/wsgi.py"
    }
  ],
  "build": {
    "env": {
      "DJANGO_SETTINGS_MODULE": "your_project.settings",
      "PYTHONPATH": "."
    },
    "command": "pip3 install -r requirements.txt && python3 manage.py collectstatic --noinput"
  }
}
